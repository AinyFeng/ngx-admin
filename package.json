  "scripts": {
    "ng": "node --max_old_space_size=12288 node_modules/@angular/cli/bin/ng",
    "conventional-changelog": "conventional-changelog",
    "serve": "npm run ng -- serve",
    "start": "npm run serve --project web",
    "s:web": "npm run serve --project web",
    "s:wx": "npm run serve --project wx",
    "s:wxpub": "npm run serve --project wxpub",
    "s:demo": "npm run serve --project demo",
    "s:coldchain": "npm run serve --project coldchain",
    "s:plan": "npm run serve -- --project plan",
    "s:vacstat": "npm run serve -- --project vaccinationstatistics",
    "s:vacstock": "npm run serve -- --project vaccinestock",
    "s:obsvaccination": "npm run serve -- --project obstetricvaccination",
    "s:vexamine": "npm run serve -- --project vaccinationexamine",
    "build": "npm run ng -- build",
    "b:liball": "npm run b:uauth && npm run b:webstorage && npm run b:common",
    "b:uauth": "npm run build -- --project uea-auth-lib",
    "b:webstorage": "npm run build -- --project ngx-webstorage",
    "b:common": "npm run build -- --project svs-common-lib",
    "b:ubase": "npm run build -- --project uea-base-lib && npm run copy-lib-assets",
    "copy-lib-assets": "npx cpx \"projects/uea-base-lib/src/lib/theme/**/*.*\" \"dist/uea-base-lib/theme/\"",
    "b:web": "npm run build -- --project web --configuration=production",
    "b:wxpub": "npm run build -- --project wxpub --configuration=production",
    "b:coldchain": "npm run build -- --project coldchain --configuration=production",
    "b:plan": "npm run build -- --project plan --configuration=production",
    "b:vacstat": "npm run build -- --project vaccinationstatistics --configuration=production",
    "b:vacstock": "npm run build -- --project vaccinestock --configuration=production",
    "b:obsvaccination": "npm run build -- --project obstetricvaccination --configuration=production",
    "b:vexamine": "npm run build -- --project vaccinationexamine --configuration=production",
    "b:demo": "npm run build -- --project demo --configuration=production",
    "r:web": "node_modules/.bin/angular-http-server -p 4200 --cors --path dist/web",
    "r:wxpub": "node_modules/.bin/angular-http-server -p 4200 --baseHref wxpub --cors --path dist/wxpub",
    "test": "npm run ng -- test -sr",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "npm run ng -- lint",
    "lint:styles": "stylelint projects/web/src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "npm run ng -- e2e",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "docs": "npm run docs:web -- --disableCoverage --disablePrivate --disableInternal",
    "docs:uauth": "compodoc -p projects/uea-auth-lib/tsconfig.lib.json -n uea-auth-lib -d dist/docs/uea-auth-lib --hideGenerator",
    "docs:web": "compodoc -p projects/web/tsconfig.app.json -n web -d dist/docs/web --hideGenerator",
    "docs:serve": "npm run docs:web -- -s",
    "docs:watch": "npm run docs:web -- -s -w",
    "format": "prettier --single-quote --write projects/**/*.ts",
    "copy:readme": "cpr README.md dist/uea-auth-lib/README.md --overwrite"
  },
